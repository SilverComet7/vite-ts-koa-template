{
  "name": "aka-gmtool",
  "version": "0.0.1",
  "main": "build/server.js",
  "scripts": {
    "dev:client": "vite",
    "serve": "vite preview",
    "dev:server": "node --nolazy -r ts-node/register src/server.ts",
    "debug:server": "ts-node-dev --inspect=24680 --respawn --poll --interval=5000 --quiet --clear src/server.ts",
    "dev": "concurrently -n vue,server npm:dev:client npm:dev:server",
    "debug": "concurrently -n vue,server npm:dev:client npm:debug:server",
    "build:client": "vue-tsc --noEmit -p view && vite build",
    "build:server": "rimraf build && tsc",
    "generate": "node generator/generateIndex.js",
    "compile": "npm run build:client && npm run build:server",
    "lint": "eslint src/**/* view/**/*.{ts,vue}",
    "format": "eslint --fix src/**/* view/**/*.{ts,vue}"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.5.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "koa": "^2.13.1",
    "koa-static": "^5.0.0",
    "log4js": "^6.3.0",
    "mongoose": "^5.10.18",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.1",
    "@types/koa-static": "^4.0.1",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.22",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "concurrently": "^6.4.0",
    "cz-conventional-emoji": "^1.0.2",
    "element-plus": "^1.2.0-beta.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.0",
    "mitt": "^3.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "vue": "^3.2.22",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.29.6"
  },
  "keywords": [
    "vue3",
    "vite2",
    "koa2",
    "mongoose",
    "typegoose"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-emoji"
    }
  }
}
