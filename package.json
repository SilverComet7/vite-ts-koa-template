{
  "name": "aka-gmtool",
  "version": "0.0.1",
  "main": "build/server.js",
  "scripts": {
    "dev:server": "node --nolazy -r ts-node/register server.ts",
    "debug:server": "ts-node-dev --inspect=24680 --respawn --poll --interval=5000 --quiet --clear server.ts",
    "dev": "concurrently -n vue,server npm:dev:client npm:dev:server",
    "debug": "concurrently -n vue,server npm:dev:client npm:debug:server",
    "build:server": "rimraf build && tsc",
    "generate": "node generator/generateIndex.js",
    "compile": "npm run build:client && npm run build:server",
    "lint": "eslint src/**/* view/**/*.{ts,vue}",
    "format": "eslint --fix src/**/* view/**/*.{ts,vue}"
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.2.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.13.2",
    "dotenv": "^10.0.0",
    "koa": "^2.13.4",
    "kcors": "^2.2.2",
    "koa-static": "^5.0.0",
    "log4js": "^6.3.0",
    "mongoose": "^6.0.13",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-static": "^4.0.2",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "mitt": "^3.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "keywords": [
    "vue3",
    "vite2",
    "koa2",
    "mongoose",
    "typegoose"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-emoji"
    }
  }
}
