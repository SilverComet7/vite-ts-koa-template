{
  "name": "publish-tool",
  "version": "0.0.1",
  "main": "build/server.js",
  "scripts": {
    "dev:client": "vite",
    "build:client": "vue-tsc --noEmit -p view && vite build",
    "serve": "vite preview",
    "dev:server": "node --nolazy -r ts-node/register src/server.ts",
    "debug:server": "ts-node-dev --inspect=24680 --respawn --poll --interval=5000 --quiet --clear src/server.ts",
    "build:server": "rimraf build && tsc",
    "dev": "concurrently -n vue,server npm:dev:client npm:dev:server",
    "debug": "concurrently -n vue,server npm:dev:client npm:debug:server",
    "compile": "npm run build:client && npm run build:server"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "koa": "^2.13.1",
    "koa-static": "^5.0.0",
    "log4js": "^6.3.0",
    "mongoose": "^5.12.5",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/koa": "^2.13.1",
    "@types/koa-static": "^4.0.1",
    "@types/mongoose": "^5.10.5",
    "@types/node": "^14.14.37",
    "@typescript-eslint/parser": "^4.20.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.7",
    "concurrently": "^6.0.0",
    "element-plus": "^1.0.2-beta.35",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.1.3",
    "vite": "^2.1.3",
    "vue": "^3.0.7",
    "vue-tsc": "^0.0.8"
  }
}
